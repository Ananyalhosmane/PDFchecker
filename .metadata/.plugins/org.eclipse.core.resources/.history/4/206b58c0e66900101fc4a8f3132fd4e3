package coder;

public class Test4 {
	public static void main (String[]args) {
        int[] pri = {7, 10, 1, 3, 6,9,2};
        int min= pri[0];
        int max = 0;

        for (int n = 1; n< pri.length; n++) {
            if (pri[n] < min) {
                min = pri[n];
            } else {
                int pro = pri[n] - min;
                if (pro > max) {
                    max = pro;
                }
            }
        }
        System.out.println("Max profit:" +max);
}
}