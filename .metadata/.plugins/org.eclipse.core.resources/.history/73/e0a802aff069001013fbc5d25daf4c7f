package coder;

import java.util.Arrays;

public class Test1 {
	public static int rotate(int[] arr) {
		int counter;
		int i;
		while (counter < k) { 
		int temp =arr[arr.length-1];
	    for (i = arr.length - 2; i >= 0; i--) {
	        arr[i + 1] = arr[i];
	    }
	    arr[0] = temp;
	    counter++;
		}
	return arr[i];
	}
public static void main (String[]args) {
	int []arr= {1,2,3,4,5};
	int k=2;
	System.out.println("the rotate:"+rotate(arr));
}
}
		