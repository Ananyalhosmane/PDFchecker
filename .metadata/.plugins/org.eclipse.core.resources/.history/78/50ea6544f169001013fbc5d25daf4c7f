package coder;

import java.util.Arrays;

public class Test1 {
	public static int[] rotateArrayByK(int[] arr, int k) {
        int counter = 0;

        while (counter < k) {
            int temp = arr[arr.length - 1];
            for (int i = arr.length - 2; i >= 0; i--) {
                arr[i + 1] = arr[i];
            }
            arr[0] = temp;
            counter++;
        }

        return arr;
    }

    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5};
        int k = 2;

        int[] result = rotateArrayByK(arr, k);
        System.out.println("Rotated Array: " + Arrays.toString(result));
    }
}
		