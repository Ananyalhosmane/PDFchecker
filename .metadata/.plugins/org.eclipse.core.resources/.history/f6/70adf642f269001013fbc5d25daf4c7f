package coder;

public class Test5 {
	    public static void main(String[] args) {
	        int[] arr = {2, 3, -8, 7, -1, 2, 3};
	        int sum = 11;

	        for (int i = 0; i < arr.length; i++) {  
	            for (int j = i + 1; j < arr.length; j++) {
	                if (arr[i] + arr[j] == sum) {
	                    System.out.println("Pair found: (" + arr[i] + ", " + arr[j] + ")");
	                }
	            }
	        }
	    }
	}
