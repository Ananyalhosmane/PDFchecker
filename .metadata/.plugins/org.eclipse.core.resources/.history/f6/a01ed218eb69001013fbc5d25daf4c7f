package coder;

import java.util.Arrays;

public class Test1 {
	public static int rotate(int[] arr) {
		int counter;
		int k;
		while (counter < k) { 
		int temp =arr[arr.length-1];
	    for (int i = arr.length - 2; i >= 0; i--) {
	        arr[i + 1] = arr[i];
	    }
	    arr[0] = temp;
	    counter++;
	}
	return arr;
	}
public static void main (String[]args) {
	int []arr=[6,1,7,3,8,9];
	int k=2;
	System.out.println("the rotation:"+arr);
	
}
		