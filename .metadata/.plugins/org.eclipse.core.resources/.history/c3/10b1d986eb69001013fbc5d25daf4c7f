package coder;

public class Test5 {
		    public static void main(String[] args) {
		        int[] arr = {6,1,7,3,8,9};
		        int min =0;
		        int max=0;
		        int first = min , second = max;

		        for (int num : arr) {
		            if (num > first) {
		                second = first;
		                first = num;
		            } else if (num > second && num != first) {
		                second = num;
		            }
		        }

		        System.out.println("Second largest: " + second);
		    }
}